struct Point {
    x: int;
    y: int;
}

def add(a: int, b: int) :: int {
    return a + b;
}

def take_point(p: Point) :: int {
    return p.x + p.y;
}

def main() :: int {
    let x: int = 5;
    let y: int = 6;
    let sum: int = add(x, y);
    let pt: Point = Point { x: 1, y: 2 };
    let pt_sum: int = take_point(pt);
    return sum + pt_sum;
}
