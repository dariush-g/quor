@import::<string.qu>;

def main() :: int {
    let filename: char* = "test.qu";
    
    // Check string length
    let len: int = strlen(filename);
    print_str("String length: ");
    print_int(len);
    print_char(10 as char);
    
    // Print all characters
    let i: int = 0;
    while (i < len) {
        let c: char = char_at(filename, i);
        print_str("Index ");
        print_int(i);
        print_str(": ");
        print_char(c);
        print_char(10 as char);
        i = i + 1;
    }

    return 0;
}
