@import::<string.qu>;

def main() :: int {
    let filename: char* = "test.qu";
    let name_len: int = 6;
    
    if (char_at(filename, name_len - 1) != 'u' || char_at(filename, name_len - 2) != 'q' || char_at(filename, name_len - 3) != '.' || is_alphabetic(char_at(filename, name_len - 4)) != true) {
        print_str("Expected <filename>.qu | Got: ");
        print_str(filename);
        print_char(10 as char);
        exit(1);
    } else {
        print_str("File extension is valid\n");
    }

    return 0;
}
