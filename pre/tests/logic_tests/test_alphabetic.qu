@import::<string.qu>;

def main() :: int {
    let filename: char* = "test.qu";
    let name_len: int = 6;
    
    // Test each character
    let c1: char = char_at(filename, name_len - 1); // 'u'
    let c2: char = char_at(filename, name_len - 2); // 'q'  
    let c3: char = char_at(filename, name_len - 3); // '.'
    let c4: char = char_at(filename, name_len - 4); // 't'
    
    print_str("c1 (u): ");
    print_char(c1);
    print_str(" is_alphabetic: ");
    print_bool(is_alphabetic(c1));
    print_char(10 as char);
    
    print_str("c2 (q): ");
    print_char(c2);
    print_str(" is_alphabetic: ");
    print_bool(is_alphabetic(c2));
    print_char(10 as char);
    
    print_str("c3 (.): ");
    print_char(c3);
    print_str(" is_alphabetic: ");
    print_bool(is_alphabetic(c3));
    print_char(10 as char);
    
    print_str("c4 (t): ");
    print_char(c4);
    print_str(" is_alphabetic: ");
    print_bool(is_alphabetic(c4));
    print_char(10 as char);

    return 0;
}
