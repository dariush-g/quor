@import::<string.qu>;

def main() :: int {
    let filename: char* = "test.qu";
    let name_len: int = 6;
    
    // Test the complex condition step by step
    let cond1: bool = char_at(filename, name_len - 1) != 'u';
    let cond2: bool = char_at(filename, name_len - 2) != 'q';
    let cond3: bool = char_at(filename, name_len - 3) != '.';
    let cond4: bool = is_alphabetic(char_at(filename, name_len - 4)) != true;
    
    print_str("cond1: ");
    print_bool(cond1);
    print_char(10 as char);
    
    print_str("cond2: ");
    print_bool(cond2);
    print_char(10 as char);
    
    print_str("cond3: ");
    print_bool(cond3);
    print_char(10 as char);
    
    print_str("cond4: ");
    print_bool(cond4);
    print_char(10 as char);
    
    // Now test the combined OR
    if (cond1 || cond2 || cond3 || cond4) {
        print_str("OR condition is TRUE\n");
    } else {
        print_str("OR condition is FALSE\n");
    }

    return 0;
}
