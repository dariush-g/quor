@import::<string.qu>;

def main() :: int {
    let a: int = 1;
    let b: int = 0;
    let c: int = 1;

    if (a == 1 || b == 1 || c == 1) {
        print_str("OR works\n");
    } else {
        print_str("OR broken\n");
